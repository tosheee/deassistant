<p id="notice"><%= notice %></p>

<h1>Listing Admin Features Brands</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Brand</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_features_brands.each do |admin_features_brand| %>
      <tr>
        <td><%= admin_features_brand.product_id %></td>
        <td><%= admin_features_brand.brand %></td>
        <td><%= admin_features_brand.description %></td>
        <td><%= link_to 'Show', admin_features_brand %></td>
        <td><%= link_to 'Edit', edit_admin_features_brand_path(admin_features_brand) %></td>
        <td><%= link_to 'Destroy', admin_features_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Features brand', new_admin_features_brand_path %>
